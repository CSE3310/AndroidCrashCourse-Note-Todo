<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

<item android:state_pressed="true" >
    <shape>
        <solid
            android:color="#F88017" />
        <stroke
            android:width="1dp"
            android:color="#E56717" />
        <corners
            android:radius="6dp" />
        <padding
            android:left="10dp"
            android:top="10dp"
            android:right="10dp"
            android:bottom="10dp" />
    </shape>
</item>
<item>
    <shape>
        <gradient
            android:startColor="#43BFC7"
            android:endColor="#3B9C9C"
            android:angle="270" />
        <stroke
            android:width="1dp"
            android:color="#307D7E" />
        <corners
            android:radius="6dp" />
        <padding
            android:left="10dp"
            android:top="10dp"
            android:right="10dp"
            android:bottom="10dp" />
    </shape>
</item>
</selector>